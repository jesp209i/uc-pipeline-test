<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="Devops" value="https://umbraco.pkgs.visualstudio.com/_packaging/Umbraco_Internal/nuget/v3/index.json" />
  </packageSources>

    <packageSourceCredentials>
    <Devops>
      <add key="Username" value="%MYGET_PASSWORD%" />
      <add key="ClearTextPassword" value="%MYGET_PASSWORD%" />
    </Devops>
  </packageSourceCredentials>

  <activePackageSource>
    <add key="All" value="(Aggregate source)" />
  </activePackageSource>
  
</configuration>
