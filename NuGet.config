<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="Umbraco_Internal" value="https://pkgs.dev.azure.com/umbraco/_packaging/Umbraco_Internal/nuget/v3/index.json" />
  </packageSources>

    <packageSourceCredentials>
    <Umbraco_Internal>
      <add key="Username" value="%UMB_USERNAME%" />
      <add key="ClearTextPassword" value="%UMB_PASSWORD%" />
    </Umbraco_Internal>
  </packageSourceCredentials>

  <activePackageSource>
    <add key="All" value="(Aggregate source)" />
  </activePackageSource>
  
</configuration>
